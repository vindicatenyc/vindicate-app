  },
  "devDependencies": {
    "@types/inquirer": "^9.0.0",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
